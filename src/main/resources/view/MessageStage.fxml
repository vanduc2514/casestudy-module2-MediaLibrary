<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="180.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.resources.controllers.MessageStage">
   <Label fx:id="message" alignment="CENTER" layoutX="120.0" layoutY="28.0" text="Message" AnchorPane.leftAnchor="50.0"
          AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="20.0">
      <font>
         <Font name="System Italic" size="20.0"/>
      </font>
   </Label>
   <TextField fx:id="userInput" layoutX="50.0" layoutY="76.0" AnchorPane.leftAnchor="50.0"
              AnchorPane.rightAnchor="50.0"/>
   <Button layoutX="169.0" layoutY="136.0" mnemonicParsing="false" onAction="#handleOkButton" prefWidth="70.0" text="OK"
           AnchorPane.rightAnchor="70.0"/>
   <Button layoutX="70.0" layoutY="135.0" mnemonicParsing="false" onAction="#closeStage" prefWidth="70.0" text="Cancel"
           AnchorPane.leftAnchor="70.0"/>
</AnchorPane>
